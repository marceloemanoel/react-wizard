{
  "name": "react-wizard",
  "version": "0.0.1",
  "description": "Simple wizard component for reactjs inspired by jquery-steps plugin",
  "main": "lib/react-wizard.min.js",
  "scripts": {
    "start": "npm-run-all --parallel dev:server lint:watch test:watch",
    "dev:server": "babel-node tools/startServer.js",
    "lint": "esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean": "npm-run-all remove-lib create-lib remove-package",
    "remove-lib": "rm -rf ./lib",
    "create-lib": "mkdir ./lib",
    "remove-package": "rm -rf react-wizard-0.0.1.tgz",
    "prebuild": "npm-run-all clean test lint",
    "build": "babel-node tools/build.js",
    "postbuild": "npm pack"
  },
  "keywords": [
    "reactjs",
    "wizard",
    "form",
    "step"
  ],
  "author": "@marceloemanoel",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "react": "^15.2.1",
    "react-addons-css-transition-group": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.9.0",
    "colors": "^1.1.2",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.1",
    "eslint-plugin-import": "^1.11.0",
    "eslint-plugin-react": "^5.2.2",
    "eslint-watch": "^2.1.13",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "jsdom": "^9.4.1",
    "mocha": "^2.5.3",
    "node-sass": "^3.8.0",
    "npm-run-all": "^2.3.0",
    "open": "0.0.5",
    "react-addons-test-utils": "^15.2.1",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.1"
  }
}
